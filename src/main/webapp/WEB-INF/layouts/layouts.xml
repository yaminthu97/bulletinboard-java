<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- Base Template -->
    <definition name="template">
        <put-attribute name="title" />
        <put-attribute name="header"
            value="/WEB-INF/pages/common/inc/header1.jsp" />
        <put-attribute name="footer"
            value="/WEB-INF/pages/common/inc/footer.jsp" />
        <put-list-attribute name="css">
            <add-attribute
                value="/resources/css/bootstrap.min.css" />
            <add-attribute
                value="/resources/css/common.css" />
        </put-list-attribute>
        <put-list-attribute name="initScripts">
            <add-attribute
                value="/resources/js/jquery-3.6.4.min.js" />
            <add-attribute
                value="/resources/js/bootstrap.bundle.min.js" />
        </put-list-attribute>
        <put-list-attribute name="scripts">
            <add-attribute value="/resources/js/common.js" />
        </put-list-attribute>
    </definition>
    <!-- Template 1 -->
    <definition name="template1" extends="template"
        template="/WEB-INF/pages/common/template1.jsp">
    </definition>
</tiles-definitions>
